fun some_function() -> Bool {
    return true
}

if some_function() {
    std::io::print_line('true') // Expect: true
}

fun early_exit() -> Number {
    return 2
    return 4
    return 8
}

std::io::print_line(early_exit())  // Expect: 2

fun nested() -> Number {
    fun inner() -> Number {
        return 1
    }

    return inner() + inner () + inner()
}

std::io::print_line(nested()) // Expect: 3