fun receiver(fn: fun()){
    fn()
}

receiver(fun x (){
    log.info('1') // Expect: 1
})

receiver(fun _ (){
    log.info('2') // Expect: 2
})

receiver(fun(){
    log.info('3') // Expect: 3
})

receiver({
    log.info('4') // Expect: 4
})

receiver(){
    log.info('5') // Expect: 5
}

receiver {
    log.info('6') // Expect: 6
}

receiver { () =>
    log.info('7') // Expect: 7
}

