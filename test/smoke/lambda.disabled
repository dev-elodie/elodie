fun receiver(fn: fun()){
    fn()
}

receiver(fun x (){
    log.info('1') // out:1
})

receiver(fun _ (){
    log.info('2') // out:2
})

receiver(fun(){
    log.info('3') // out:3
})

receiver({
    log.info('4') // out:4
})

receiver(){
    log.info('5') // out:5
}

receiver {
    log.info('6') // out:6
}

receiver { () =>
    log.info('7') // out:7
}

